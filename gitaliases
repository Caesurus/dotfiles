[alias]
      aa = add --all
      ao = add --patch
      ca = commit --amend
      co = checkout
      ci = commit
      cp = cherry-pick
      st = status
      br = branch
      hist = log --pretty=format:\"%h %cd | %s%d [%an]\" --graph --date=short
      d = diff
      dc = diff --cached
      type = cat-file -t
      dump = cat-file -p

