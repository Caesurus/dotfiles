[alias]
      aa = add --all
      ao = add --patch
      ca = commit --amend
      co = checkout
      ci = commit
      cp = cherry-pick
      st = status
      sl = stash list
      sp = stash pop
      br = branch
      hist = log --pretty=format:\"%h %cd | %s%d [%an]\" --graph --date=short
      d = diff
      dc = diff --cached
      rc = rebase --continue
      ra = rebase --abort
      rs = rebase --skip
      type = cat-file -t
      dump = cat-file -p

